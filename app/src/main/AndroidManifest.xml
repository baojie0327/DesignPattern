<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ahuang.designpattern">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".strategymode.StrategyModeActivity"></activity>
        <activity android:name=".statemode.StateModeActivity" />
        <activity android:name=".observermode.ObserverActivity" />
        <activity android:name=".memotomode.MemotoActivity" />
        <activity android:name=".mediator.MediatorActivity" />
        <activity android:name=".commandmode.CommandModeActivity" />
        <activity android:name=".responsiblechain.ResponsibilityChainActivity" />
        <activity android:name=".proxymode.ProxyActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".singleton.SingletonActivity" />
        <activity android:name=".SimpleFactory.SimpleFactoryActivity" />
        <activity android:name=".factorymethod.FactoryMethodActivity" />
        <activity android:name=".abstractfactory.AbstractFactoryActivity" />
        <activity android:name=".builderpattern.BuilderActivity" />
        <activity android:name=".prototype.PrototypeActivity" />
        <activity android:name=".adaptermode.AdapterActivity" />
        <activity android:name=".bridge.BridgeActivity" />
        <activity android:name=".decoratemode.DecorateActivity" />
        <activity android:name=".facademode.FacadeActivity" />
        <activity android:name=".flyweight.FlyweightActivity" />
    </application>

</manifest>